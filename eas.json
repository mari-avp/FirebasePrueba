{
  "cli": {
    "version": ">= 14.3.1",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android":{
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType":"app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
